  "components": {
    "schemas": {
      "ApiUser": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "user": {
            "type": "string",
            "nullable": true
          },
          "password": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "Finalidade": {
        "enum": [
          "DECORACAO",
          "EMBALAGEM",
          "PALETIZACAO",
          "TODOS"
        ],
        "type": "string"
      },
      "Image": {
        "required": [
          "finalidade",
          "imagePath",
          "productId"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "productId": {
            "maxLength": 10,
            "minLength": 0,
            "type": "string"
          },
          "imagePath": {
            "maxLength": 500,
            "minLength": 0,
            "type": "string"
          },
          "sequence": {
            "type": "integer",
            "format": "int32"
          },
          "imageMain": {
            "type": "boolean"
          },
          "finalidade": {
            "minLength": 1,
            "type": "string"
          },
          "product": {
            "$ref": "#/components/schemas/Product"
          }
        },
        "additionalProperties": false
      },
      "Product": {
        "required": [
          "productId"
        ],
        "type": "object",
        "properties": {
          "productId": {
            "maxLength": 10,
            "minLength": 1,
            "type": "string"
          },
          "productName": {
            "maxLength": 255,
            "type": "string",
            "nullable": true
          },
          "barcode": {
            "maxLength": 20,
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      }
